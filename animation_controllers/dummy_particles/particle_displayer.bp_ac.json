                    "transitions": [{ "update": "q.property('xp:control') != 'none'" }]
                },
                "particle_11": {
                    "on_entry": [ "/event entity @s xp:particle_11" ],
                    "transitions": [{ "update": "q.property('xp:control') != 'none'" }]
                },
                "particle_12": {
                    "on_entry": [ "/event entity @s xp:particle_12" ],
                    "transitions": [{ "update": "q.property('xp:control') != 'none'" }]
                },
                "particle_13": {
                    "on_entry": [ "/event entity @s xp:particle_13" ],
                    "transitions": [{ "update": "q.property('xp:control') != 'none'" }]
                }
            }
        }
    }
}