{
  "format_version": "1.10.0",
  "animation_controllers": {
    "controller.animation.void_armor.helmet": {
      "initial_state": "default",
      "states": {
        "default": {
          "transitions": [ { "has_armor_on": "q.armor_texture_slot(0) == 1" } ]
        },
        "has_armor_on": {
          "animations": [ "void_armor.powers" ],
          "transitions": [ { "default": "q.armor_texture_slot(0) != 1" } ],
          "on_entry": [
            "/playsound xp_armor.equip @a[r=25] ~~~ 0.75 0.6 0.01",
            "/particle xp:storm_armor.helmet ~~1.65~"
          ],
          "on_exit": [ "/playsound xp_armor.equip_1 @a[r=25] ~~~ 0.25 1.5 0.01" ]
        }
      }
    },
