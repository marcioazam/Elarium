{
  "format_version": "1.16.0",
  "minecraft:entity": {
    "description": {
      "identifier": "xp:warder",
      "is_spawnable": true,
      "is_summonable": true,

      "animations": {
        "player.achievements": "controller.animation.player.achievements.mob_death_detection",

        "root.warder.teleport": "controller.animation.warder.teleport",
        "root.warder.death": "controller.animation.warder.death",
        "teleport_random": "animation.warder.teleport_random",
        "warder.despawn": "animation.warder.despawn",
        "broken_sanctuary_fast_despawn": "animation.common.broken_sanctuary_fast_despawn",
        "ender_ruins_despawn_on_boss_arena": "animation.common.ender_ruins_despawn_on_boss_arena"
      },

      "scripts": {
        "animate": [
          "player.achievements",

          { "root.warder.teleport": "!q.is_baby" },
          "root.warder.death",
          "broken_sanctuary_fast_despawn",
          "ender_ruins_despawn_on_boss_arena",
          { "teleport_random": "!q.has_target && !q.is_baby" },
          { "warder.despawn": "q.is_baby && q.is_angry"}
        ]
      }
    },
    "component_groups": {
      //default mode
      "xp:stage.teleport": {
        "minecraft:is_tamed": {},
        "minecraft:environment_sensor": {
          "triggers": [
            {
              "event": "xp:stage_no_teleport",
              "target": "self",
              "filters": {
                "test": "in_caravan",
                "value": false
              }
            }
          ]
        }
      },
      "xp:stage.teleport_hit": {
        "minecraft:is_saddled": {},
        "minecraft:environment_sensor": {
          "triggers": [
            {
              "event": "xp:stage_teleport",
              "target": "self",
              "filters": {
                "test": "in_caravan",
                "value": false
              }
            }
          ]
        }
      },

      //angry mode
      "xp:stage.calm": {
        "minecraft:spell_effects": {
          "remove_effects": ["slowness"]
        },
        "minecraft:entity_sensor": {
          "minimum_count": 1,
          "relative_range": true,
          "require_all": false,
          "sensor_range": 3,
          "event": "xp:stage_angry",
          "event_filters": [
            {
              "all_of": [
                {
                  "test": "is_family",
                  "subject": "other",
                  "operator": "==",
                  "value": "player"
                },
                {
                  "test": "has_component",
                  "subject": "self",
                  "operator": "!=",
                  "value": "minecraft:is_baby"
                }
              ]
            }
          ]
        }
      },
      "xp:stage.pre_attack": {
        "minecraft:skin_id": {
          "value": 1
        },
        "minecraft:timer": {
          "time": 0.8,
          "looping": false,
          "time_down_event": {
            "event": "xp:stage_angry",
            "target": "self"
          }
        },
        "minecraft:behavior.nearest_attackable_target": {
          "priority": 1,
          "must_see": true,
          "reselect_targets": true,
          "must_see_forget_duration": 40,
          "entity_types": [
            {
              "filters": {
                "all_of": [
                  {
                    "test": "is_family",
                    "subject": "other",
                    "value": "player"
                  },
                  {
                    "test": "has_component",
                    "subject": "self",
                    "operator": "!=",
                    "value": "minecraft:is_baby"
                  }
                ]
              },
              "max_dist": 20
            }
          ]
        },
        "minecraft:spell_effects": {
          "add_effects": [
            {
              "ambient": false,
              "amplifier": 255,
              "display_on_screen_animation": false,
              "duration": 999,
              "effect": "slowness",
              "visible": false
            }
          ]
        },
        "minecraft:angry": {
          "duration": 1,
          "broadcast_anger": true,
          "broadcast_range": 22
        },

        "minecraft:behavior.ranged_attack": {
          "priority": 1,
          "attack_interval": 999,
          "attack_radius": 22,
          "speed_multiplier": 0
        },
        "minecraft:shooter": {
          "def": "minecraft:arrow"
        }
      },
      "xp:stage.angry": {
        "minecraft:skin_id": {
          "value": 2
        },
        "minecraft:angry": {
          "duration": 25,
          "broadcast_anger": true,
          "broadcast_range": 10,
          "broadcast_filters": {
            "test": "has_component",
            "subject": "self",
            "operator": "!=",
            "value": "minecraft:is_baby"
          },
          "calm_event": {
            "event": "xp:stage_calm",
            "target": "self"
          }
        },
        "minecraft:spell_effects": {
          "remove_effects": ["slowness"]
        },
        "minecraft:behavior.nearest_attackable_target": {
          "priority": 1,
          "must_see": true,
          "reselect_targets": true,
          "entity_types": [
            {
              "filters": {
                "all_of": [
                  {
                    "test": "is_family",
                    "subject": "other",
                    "value": "player"
                  },
                  {
                    "test": "has_component",
                    "subject": "self",
                    "operator": "!=",
                    "value": "minecraft:is_baby"
                  }
                ]
              },
              "max_dist": 20
            }
          ]
        },
        "minecraft:on_target_escape": {
          "target": "self",
          "event": "xp:stage_calm"
        },

        "minecraft:behavior.melee_attack": {
          "speed_multiplier": 2.7
        },
        "minecraft:attack": {
          "damage": 4
        }
      },

      "xp:lookat": {
        "minecraft:is_illager_captain": {}
      },

      //death mode
      "xp:stage.death": {
        "minecraft:type_family": {
          "family": ["inanimate"]
        },
        "minecraft:collision_box": {
          "width": 0,
          "height": 0
        },
        "minecraft:spawn_entity": {
          "entities": [
            {
              "single_use": true,
              "max_wait_time": 0,
              "min_wait_time": 0,
              "num_to_spawn": 1,
              "spawn_item": "xp:ender_chips"
            }
          ]
        },
        "minecraft:damage_sensor": {
          "triggers": [
            {
              "cause": "all",
              "deals_damage": false
            }
          ]
        },
        "minecraft:is_baby": {},
        "minecraft:spell_effects": {
          "add_effects": [
            {
              "ambient": false,
              "amplifier": 255,
              "duration": 999,
              "effect": "slowness",
              "visible": false
            }
          ]
        }
      },
      "xp:instant_despawn": {
        "minecraft:instant_despawn": {
          "remove_child_entities": false
        }
      }
    },

    "components": {
      "minecraft:is_hidden_when_invisible": {},
      "minecraft:experience_reward": {
        "on_death": "query.last_hit_by_player ? 2 : 0"
      },
      "minecraft:type_family": {
        "family": ["warder", "monster", "mob"]
      },
      "minecraft:breathable": {
        "total_supply": 15,
        "suffocate_time": 0
      },
      "minecraft:nameable": {},
      //heath == 10
      "minecraft:health": {
        "value": 30,
        "max": 30
      },
      "minecraft:custom_hit_test": {
        "hitboxes": [
          {
            "width": 0.7,
            "height": 2.4,
            "pivot": [0, 1.2, 0]
          }
        ]
      },
      "minecraft:lookat": {
        "search_radius": 20.0,
        "allow_invulnerable": false,
        "set_target": true,
        "look_cooldown": 0,
        "look_event": {
          "event": "xp:lookat",
          "target": "self"
        },
        "filters": {
          "all_of": [
            {
              "subject": "other",
              "test": "is_family",
              "value": "player"
            },
            {
              "test": "has_component",
              "subject": "self",
              "operator": "!=",
              "value": "minecraft:is_baby"
            },
            {
              "test": "has_equipment",
              "domain": "head",
              "subject": "other",
              "operator": "not",
              "value": "carved_pumpkin"
            }
          ]
        }
      },
      "minecraft:on_target_acquired": {
        "target": "self",
        "event": "xp:attack_random"
      },
      "minecraft:on_target_escape": {
        "event": "xp:stage_calm",
        "target": "self"
      },
      "minecraft:hurt_on_condition": {
        "damage_conditions": [
          {
            "filters": {
              "test": "in_contact_with_water",
              "operator": "==",
              "value": true
            },
            "cause": "drowning",
            "damage_per_tick": 1
          }
        ]
      },
      "minecraft:environment_sensor": {
        "triggers": [
          {
            "event": "xp:stage_death",
            "target": "self",
            "filters": {
              "test": "has_component",
              "subject": "self",
              "operator": "==",
              "value": "minecraft:is_baby"
            }
          }
        ]
      },
      "minecraft:follow_range": {
        "value": 22,
        "max": 22
      },
      "minecraft:despawn": {
        "despawn_from_distance": {}
      },
      "minecraft:damage_sensor": {
        "triggers": [
          {
            "cause": "projectile",
            "on_damage": {
              "filters": {
                "test": "is_family",
                "subject": "damager",
                "operator": "!=",
                "value": "sonic_beam"
              },
              "target": "self",
              "event": "xp:stage_teleport_hit"
            },
            "deals_damage": false
          },
          {
            "cause": "fire",
            "deals_damage": false
          },
          {
            "cause": "entity_attack",
            "on_damage": {
              "filters": [
                {
                  "all_of": [
                    {
                      "test": "has_equipment",
                      "subject": "other",
                      "domain": "hand",
                      "value": "xp:the_touched_ends"
                    }
                  ]
                }
              ],
              "target": "other",
              "event": "xp:regeneration_value_1"
            },
            "deals_damage": true
          }
        ]
      },
      "minecraft:movement": {
        "value": 0.2
      },
      "minecraft:navigation.walk": {
        "can_path_over_water": false,
        "avoid_water": true
      },
      "minecraft:movement.basic": {},
      "minecraft:jump.static": {},
      "minecraft:can_climb": {},
      "minecraft:pushable": {
        "is_pushable": true,
        "is_pushable_by_piston": true
      },
      "minecraft:physics": {
        "has_collision": true,
        "has_gravity": true
      },

      "minecraft:behavior.float": {
        "priority": 5
      },
      "minecraft:behavior.random_stroll": {
        "priority": 8,
        "speed_multiplier": 1.0
      },
      "minecraft:behavior.look_at_player": {
        "priority": 8,
        "look_distance": 8.0,
        "probability": 8.0
      },
      "minecraft:behavior.hurt_by_target": {
        "priority": 3
      },
      "minecraft:behavior.avoid_block": {
        "priority": 6,
        "walk_speed_modifier": 1.4,
        "tick_interval": 1,
        "search_range": 6,
        "search_height": 6,
        "target_selection_method": "nearest",
        "target_blocks": [
          "minecraft:water",
          "minecraft:flowing_water",
          "minecraft:coral_block",
          "minecraft:coral_fan",
          "coral_fan_dead"
        ]
      },
      "minecraft:conditional_bandwidth_optimization": {
        "default_values": {
          "max_optimized_distance": 80.0,
          "max_dropped_ticks": 10,
          "use_motion_prediction_hints": true
        }
      }
    },

    "events": {
      //default event
      "minecraft:entity_spawned": {
        "sequence": [
          {
            "trigger": "xp:stage_calm"
          }
        ]
      },
      "xp:stage_teleport": {
        "add": {
          "component_groups": ["xp:stage.teleport"]
        }
      },
      "xp:stage_remove_teleport": {
        "remove": {
          "component_groups": ["xp:stage.teleport"]
        }
      },

      "xp:stage_teleport_hit": {
        "add": {
          "component_groups": ["xp:stage.teleport_hit"]
        }
      },
      "xp:stage_remove_teleport_hit": {
        "remove": {
          "component_groups": ["xp:stage.teleport_hit"]
        }
      },

      "xp:lookat": {
        "add": {
          "component_groups": ["xp:lookat"]
        },
        "trigger": "xp:stage_angry"
      },
      "xp:remove_lookat": {
        "remove": {
          "component_groups": ["xp:lookat"]
        }
      },

      //angry event
      "xp:stage_calm": {
        "add": {
          "component_groups": ["xp:stage.calm"]
        },
        "remove": {
          "component_groups": ["xp:stage.angry", "xp:stage.pre_attack"]
        }
      },
      "xp:attack_random": {
        "sequence": [
          {
            "filters": {
              "test": "has_component",
              "subject": "self",
              "operator": "==",
              "value": "minecraft:is_illager_captain"
            },
            "trigger": "xp:stage_angry"
          },
          {
            "filters": {
              "test": "has_component",
              "subject": "self",
              "operator": "!=",
              "value": "minecraft:is_illager_captain"
            },
            "randomize": [
              {
                "trigger": "xp:stage_pre_attack",
                "weight": 2
              },
              {
                "trigger": "xp:stage_angry",
                "weight": 2
              }
            ]
          }
        ]
      },
      "xp:stage_pre_attack": {
        "add": {
          "component_groups": ["xp:stage.pre_attack"]
        },
        "remove": {
          "component_groups": ["xp:stage.angry", "xp:stage.calm"]
        }
      },
      "xp:stage_angry": {
        "add": {
          "component_groups": ["xp:stage.angry"]
        },
        "remove": {
          "component_groups": [
            "xp:stage.pre_attack",
            "xp:stage.calm",
            "xp:stage.teleport",
            "xp:stage.teleport_cooldown"
          ]
        }
      },

      //death
      "xp:stage_death": {
        "add": {
          "component_groups": ["xp:stage.death"]
        },
        "remove": {
          "component_groups": [
            "xp:stage.teleport",
            "xp:stage.teleport_hit",
            "xp:stage.calm",
            "xp:stage.pre_attack",
            "xp:stage.angry"
          ]
        }
      },
      "xp:instant_despawn": {
        "add": {
          "component_groups": ["xp:instant_despawn"]
        }
      }
    }
  }
}
